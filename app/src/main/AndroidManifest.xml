<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SampleCompose">
        <activity
            android:name=".lazy.LazyRowAndColumn16Activity"
            android:exported="true"
            android:label="@string/title_activity_lazy_row_and_column16"
            android:theme="@style/Theme.SampleCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StateReComposition10Activity"
            android:exported="true"
            android:theme="@style/Theme.SampleCompose" />
        <activity
            android:name=".CalculatorActivity"
            android:exported="false"
            android:label="@string/title_activity_calculator"
            android:theme="@style/Theme.SampleCompose" />
        <activity
            android:name=".DiceRollerActivity"
            android:exported="true"
            android:label="@string/title_activity_dice_roller"
            android:theme="@style/Theme.SampleCompose" />
        <activity
            android:name=".PracticeComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_practice_compose"
            android:theme="@style/Theme.SampleCompose" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.SampleCompose" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>